/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a=a, b=b, out=x);
    And(a=a, b=b, out=andab);
    Not(in=andab, out=y);
    And(a=x, b=y, out=out);
}